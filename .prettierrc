{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "importOrder": [
    "^react$",
    "^next.*$",
    "<THIRD_PARTY_MODULES>",
    "^@/types.*$",
    "^@/app.*$",
    "^@/lib.*$",
    "^@/hooks.*$",
    "^@/components.*$",
    "^[./]",
    "^.*.css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
